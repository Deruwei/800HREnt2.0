<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:stretchColumns="1" >
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#f6f6f6" />

    <RelativeLayout
        android:id="@+id/rl_myresume_1"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:background="#EEEEEE"
        android:gravity="center_vertical"
        >

        <ImageView
            android:id="@+id/imageView43"
            android:layout_width="7dp"
            android:layout_height="16dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:src="@mipmap/jianli_yulan_sutiao" />

        <TextView
            android:id="@+id/selfevaluation_title"
            style="@style/resume_previewresume_title"
            android:layout_centerVertical="true"
            android:layout_marginLeft="6dp"
            android:layout_marginStart="6dp"
            android:layout_toEndOf="@+id/imageView43"
            android:layout_toRightOf="@+id/imageView43"
            android:text="@string/selfevaluation_title" />
    </RelativeLayout>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:background="@android:color/white"
        android:padding="5dp" >
        
       
        <TextView
            android:id="@+id/selfevaluation_desc"
            style="@style/colour_style2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:layout_span="2" />
    </TableRow>

</TableLayout>