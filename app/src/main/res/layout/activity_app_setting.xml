<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F6F6F6"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/rl_settingapp_top"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="#F39800">

        <ImageView
            android:id="@+id/iv_settingapp_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/topfanhui" />

        <TextView
            android:id="@+id/tv_settingapp_jobnum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="设置"
            android:textColor="#fff"
            android:textSize="20sp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#d3d3d3" />

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <RelativeLayout
        android:id="@+id/rl_settingapp_contact"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#fff">

        <TextView
            android:id="@+id/textView372333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="联系我们"
            android:textColor="#666666"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_settingapp_attention"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />

        <View
            android:layout_width="360dp"
            android:layout_height="1dp"
            android:layout_alignLeft="@+id/textView372333"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/textView372333"
            android:background="#EBEBEB" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_settingapp_feedback"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#fff">

        <TextView
            android:id="@+id/textView372"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="给我们打分"
            android:textColor="#666666"
            android:textSize="16sp" />
        <ImageView
            android:id="@+id/iv_settingapp_autologin111"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />

    </RelativeLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <RelativeLayout

        android:id="@+id/rl_settingapp_apk"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#fff">

        <TextView
            android:id="@+id/textView373"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="行业找工作APP下载"
            android:textColor="#666666"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_settingapp_autologin3"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />

        <View
            android:layout_width="360dp"
            android:layout_height="1dp"
            android:layout_alignLeft="@+id/textView373"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/textView373"
            android:background="#EBEBEB" />
    </RelativeLayout>

    <RelativeLayout

        android:id="@+id/rl_settingapp_hotadv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#fff">

        <TextView
            android:id="@+id/textView3736"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="移动热点广告展示"
            android:textColor="#666666"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_settingapp_comregister"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />

        <View
            android:layout_width="360dp"
            android:layout_height="1dp"
            android:layout_alignLeft="@+id/textView3736"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/textView3736"
            android:background="#EBEBEB" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_settingapp_brand"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#fff">

        <TextView
            android:id="@+id/textView3737"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:text="移动品牌广告展示"
            android:textColor="#666666"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_settingapp_mark"
            android:layout_width="6dp"
            android:layout_height="12dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:src="@mipmap/jiantou_right" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <RelativeLayout
        android:id="@+id/rl_settingapp_loginout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#fff">

        <TextView
            android:id="@+id/textView374"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:text="退出登录"
            android:textColor="#666666"
            android:textSize="16sp" />
    </RelativeLayout>
</LinearLayout>
