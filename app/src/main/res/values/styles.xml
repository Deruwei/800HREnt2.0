        <item name="android:textColor">#F39D0D</item>
        <item name="android:textSize">13sp</item>
        <item name="android:button">@drawable/check_bg2</item>
    </style>

    <style name="linear_dointencomm_tv">
        <item name="android:layout_height">wrap_content</item>
